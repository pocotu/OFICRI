{
  "name": "oficri",
  "version": "1.0.0",
  "description": "Sistema de GestiÃ³n para OFICRI Cusco - ISO/IEC 27001 compliant",
  "main": "server/server.js",
  "scripts": {
    "start": "node start.js",
    "dev": "nodemon start.js",
    "mock": "node server/mock-server.js",
    "start:client": "webpack serve --mode development",
    "build:client": "webpack --mode production",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --reporters='default' --reporters='jest-junit'",
    "test:custom": "node server/scripts/run-tests.js",
    "test:auth": "jest server/tests/auth.test.js --runInBand",
    "test:api": "jest server/tests/api.test.js --runInBand",
    "test:db": "jest server/tests/database.test.js --runInBand",
    "test:logger": "jest server/tests/logger.test.js --runInBand",
    "test:integration": "jest server/tests/integration.test.js --runInBand",
    "test:entity": "jest server/tests/entity/*.test.js --runInBand",
    "test:entity:usuario": "node server/scripts/run-entity-tests.js usuario",
    "test:entity:area": "node server/scripts/run-entity-tests.js area",
    "test:entity:documento": "node server/scripts/run-entity-tests.js documento",
    "test:entity:derivacion": "node server/scripts/run-entity-tests.js derivacion",
    "setup:test-data": "node server/scripts/setup-test-data.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,css,html}\"",
    "audit": "npm audit",
    "backup": "node server/scripts/backup/backup.js",
    "security-check": "node server/scripts/security-audit.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run start:client\"",
    "dev:mock": "concurrently \"npm run mock\" \"npm run start:client\"",
    "db:init": "node server/scripts/run-sql-file.js -f db/db.sql",
    "db:triggers": "node server/scripts/run-sql-file.js -f db/trigger-control.sql",
    "db:setup": "node server/scripts/setup.js",
    "init": "npm run db:init && npm run db:triggers && npm run db:setup",
    "check:db": "node server/scripts/check-database.js",
    "init:db": "node server/scripts/init-database.js",
    "db:crear-admin": "node server/scripts/crear-admin.js",
    "db:limpiar": "node server/scripts/limpiar-db.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.3.0",
    "bootstrap-icons": "^1.10.0",
    "compression": "^1.8.0",
    "connect-redis": "^8.0.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "express-list-endpoints": "^7.1.1",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "express-validator": "^7.0.1",
    "filepond": "^4.32.7",
    "helmet": "^7.2.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.6.0",
    "ip-info-finder": "^3.0.3",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "mysql2": "^3.14.0",
    "punycode2": "^1.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "babel-loader": "^9.1.2",
    "concurrently": "^9.1.2",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^6.8.0",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^2.1.1",
    "html-webpack-plugin": "^5.5.1",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jscpd": "^4.0.5",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "style-loader": "^3.3.3",
    "supertest": "^6.3.4",
    "terser-webpack-plugin": "^5.3.7",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "oficri",
    "api",
    "security",
    "iso27001"
  ],
